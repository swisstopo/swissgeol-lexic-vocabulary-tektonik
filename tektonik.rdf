	<skos:broader rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/Grueenhorn-CasannaSliceComplex"/>
	<status xmlns="http://www.w3.org/ns/adms#" rdf:resource="https://dev-lexic.swissgeol.ch/active"/>
