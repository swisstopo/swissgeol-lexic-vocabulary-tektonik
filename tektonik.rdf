	<skos:prefLabel xml:lang="fr">Nappe de l'Ortler</skos:prefLabel>
	<skos:prefLabel xml:lang="it">Falda dell'Ortler</skos:prefLabel>
	<skos:broader rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/UpperAustroalpine"/>
