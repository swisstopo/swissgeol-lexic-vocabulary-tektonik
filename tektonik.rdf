	<skos:prefLabel xml:lang="en">Internal Einsiedeln Slices</skos:prefLabel>
	<skos:prefLabel xml:lang="de">Interne Einsiedeln-Schuppen</skos:prefLabel>
	<skos:prefLabel xml:lang="fr">Écailles internes d'Einsiedeln</skos:prefLabel>
	<skos:prefLabel xml:lang="it">Scaglie interne di Einsiedeln</skos:prefLabel>
