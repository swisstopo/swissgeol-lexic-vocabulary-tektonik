	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/LowerOrobicImbricates"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/UpperOrobicNappe"/>
