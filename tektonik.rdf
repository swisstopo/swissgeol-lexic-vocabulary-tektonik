	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/GotschnawangSlice"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/DrosSlice"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/TotalpOphioliteComplex"/>
