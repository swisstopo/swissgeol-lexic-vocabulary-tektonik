	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/PrealpesMedianesPlastiques"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/PrealpesMedianesRigides"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/Mythen-RoggeneggSlice"/>
