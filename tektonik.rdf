	<skos:broader rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/UpperHelvetic"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/InfraprealpineMelange"/>
