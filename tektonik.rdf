	<skos:prefLabel xml:lang="it">Scaglie Orobiche inferiore</skos:prefLabel>
	<skos:altLabel xml:lang="de">Untere Orobische Decke</skos:altLabel>
	<skos:altLabel xml:lang="fr">Nappe Orobique inférieure</skos:altLabel>
	<skos:broader rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/SouthAlpine"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/InternalGiudicarieZone"/>
