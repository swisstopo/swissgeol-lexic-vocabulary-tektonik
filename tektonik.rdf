	<skos:prefLabel xml:lang="de">Zone Houillère interne</skos:prefLabel>
	<skos:prefLabel xml:lang="fr">Zone Houillère interne</skos:prefLabel>
	<skos:prefLabel xml:lang="it">Zona Houillère interna</skos:prefLabel>
