</rdf:Description>

<rdf:Description rdf:about="https://dev-lexic.swissgeol.ch/TectonicUnits/Bardella-RoccabellaSlices">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
