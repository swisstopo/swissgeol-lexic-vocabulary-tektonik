	<skos:prefLabel xml:lang="fr">Écailles de Chropfsberg-Pizalun</skos:prefLabel>
	<skos:prefLabel xml:lang="it">Scaglie di Chropfsberg-Pizalun</skos:prefLabel>
	<skos:broader rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/LowerHelvetic"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/ChropfsbergSlice"/>
	<skos:narrower rdf:resource="https://dev-lexic.swissgeol.ch/TectonicUnits/PizalunSlice"/>
